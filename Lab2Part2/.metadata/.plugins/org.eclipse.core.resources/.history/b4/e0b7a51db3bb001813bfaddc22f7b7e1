
public class TimeTable {
	public static void main(String[] args) {
		System.out.println("* |  1  2  3  4  5  6  7  8  9");
		System.out.println("---------------------------------------");
		for(int i=1;i<10;i++) {
			for(int x=1;x<10;x++) {
				System.out.print(x*i);
				System.out.print("  ");
			}
			System.out.println(i+" | ");
			
		}
	}
}
