public class Start {
	public static void main(String[] args) {
		Student.testStudent();	
		Student s1=new Student(1730004002);
		System.out.println(check(s1)=="need coffee");
		s1.fallAsleep();
		System.out.println(check(s1)=="sweet dream");
		}
	
	public static String check(Student s) {
		if(s.isSleeping()==false) 
			return "need coffee";
		
		else 
			return "sweet dream";
		
		
		}
	

	
	
	
}
